import { ethers } from "ethers";
import abi from "./AIAgentWalletABI.json" assert { type: "json" };

const provider = new ethers.JsonRpcProvider("https://rpc-url");
const aiPrivateKey = "YOUR_AI_PRIVATE_KEY";
const wallet = new ethers.Wallet(aiPrivateKey, provider);

const contract = new ethers.Contract(
  "WALLET_CONTRACT_ADDRESS",
  abi,
  wallet
);

// Example: send ETH
async function sendETH() {
  const tx = await contract.execute(
    "0xRecipientAddress",
    ethers.parseEther("0.01"),
    "0x"
  );

  await tx.wait();
  console.log("AI executed transfer");
}

sendETH();
